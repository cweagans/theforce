{
  "name": "cweagans/theforce",
  "description": "An autocomplete library for PHP.",
  "authors": [
    {
      "name": "Cameron Eagans",
      "email": "me@cweagans.net"
    }
  ],
  "require": {
    "nikic/php-parser": "dev-errorRecovery2",
    "phpdocumentor/reflection-docblock": "2.0.4",
    "react/socket": "0.4.*@dev"
  },
  "bin": [
    "theforce"
  ],
  "require-dev": {
    "phpunit/phpunit": "4.5.0"
  },
  "autoload": {
    "psr-4": {"cweagans\\TheForce\\": "src"}
  },
  "autoload-dev": {
    "psr-4": {"cweagans\\TheForce\\Tests\\": "test"}
  }
}
